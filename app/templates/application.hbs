<div class="wrapper">

  <!-- Main Header -->
  <header class="main-header">

    <!-- Logo -->
    {{#link-to "index" class="logo" }}
      <!-- mini logo for sidebar mini 50x50 pixels -->
      <span class="logo-mini"><b>PUSH</b></span>
      <!-- logo for regular state and mobile devices -->
      <span class="logo-lg"><b>Push</b>TJDFT</span>
    {{/link-to}}

    <!-- Header Navbar -->
    <nav class="navbar navbar-static-top">

      <!-- Sidebar toggle button-->
      <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
        <span class="sr-only">Toggle navigation</span>
      </a>
      <!-- Navbar Right Menu -->
      <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">
          <!-- Messages: style can be found in dropdown.less-->

          <!-- Notifications: style can be found in dropdown.less -->

          <!-- Tasks: style can be found in dropdown.less -->

          <!-- User Account: style can be found in dropdown.less -->

          <!-- Control Sidebar Toggle Button -->
          <li>
            <button class="btn btn-default" data-toggle="control-sidebar">Toggle Right Sidebar</button>

          </li>
          <li>
            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
          </li>
        </ul>
      </div>
    </nav>
  </header>
  <!-- Left side column. contains the logo and sidebar -->
  <aside class="main-sidebar">

    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">

      <!-- Sidebar user panel (optional) -->


      <!-- search form (Optional) -->
      <form action="#" method="get" class="sidebar-form">
        <div class="input-group">
          <input type="text" name="q" class="form-control" placeholder="Search...">
          <span class="input-group-btn">
                <button type="submit" name="search" id="search-btn" class="btn btn-flat"><i class="fa fa-search"></i>
                </button>
              </span>
        </div>
      </form>
      <!-- /.search form -->

      <!-- Sidebar Menu -->
      <ul class="sidebar-menu">
        <li class="header">Menu</li>
        <!-- Optionally, you can add icons to the links -->
        <!--li >{{#link-to "itens"}}<i class="fa fa-envelope"></i><span>E-mails enviados</span>{{/link-to}}</li>
        <li>{{#link-to "processos" }}<i class="fa fa-gavel"></i> <span>Processo Judicial</span>{{/link-to}}</li>
        <li>{{#link-to "pads"}}<i class="fa fa-bank"></i> <span> Processo Administrativo</span>{{/link-to}}</li-->
        {{#link-to "itens" tagName="li" href=false }}
          <a {{bind-attr href="itens"}}>
            <i class="fa fa-envelope"></i><span>E-mails enviados</span>
          </a>
        {{/link-to}}
        {{#link-to "processos" tagName="li" href=false }}
          <a {{bind-attr href="itens"}}>
            <i class="fa fa-gavel"></i><span>Processo Judicial</span>
          </a>
        {{/link-to}}
        {{#link-to "pads" tagName="li" href=false }}
          <a {{bind-attr href="itens"}}>
            <i class="fa fa-bank"></i><span>Processo Administrativo</span>
          </a>
        {{/link-to}}
      </ul>
      <!-- /.sidebar-menu -->
    </section>
    <!-- /.sidebar -->
  </aside>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Page header
        <small>Optional description</small>
      </h1> {{bread-crumbs tagName="ol" class="breadcrumb" linkable=true}}

      <!--ol class="breadcrumb">
        <li>{{#link-to "index"}}Início{{/link-to}}</li>
        {{bread-crumbs tagName="li" outputStyle="bootstrap" linkable=true}}
      </ol-->
    </section>

    <!-- Main content -->
    <section class="content">

      <!-- Your Page Content Here -->
      {{outlet}}
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <!-- Main Footer -->
  <footer class="main-footer">
    <!-- To the right -->
    <div class="pull-right hidden-xs">
      O que colocar aqui?
    </div>
    <!-- Default to the left -->
    <strong>2016 - <a href="#">TJDFT</a></strong>
  </footer>

  <!-- Control Sidebar -->
  <!-- The Right Sidebar -->
  <aside class="control-sidebar control-sidebar-light">
    <!-- Content of the sidebar goes here -->
    <li>
      {{#link-to "dados-pessoais"}}
        <span class="glyphicon glyphicon-user" aria-hidden="true"></span> Dados pessoais{{/link-to}}
    </li>
    <li>
      {{#link-to "senha"}}
        <span class="glyphicon glyphicon-lock" aria-hidden="true"></span> Alterar senha{{/link-to}}
    </li>
    <li>
      {{#link-to "exclusao"}}
        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span> Excluir usuário{{/link-to}}
    </li>
    <li role="separator" class="divider"></li>
    <li>
      {{#link-to "sair"}}
        <span class="glyphicon glyphicon-off"></span> Sair
      {{/link-to}}
    </li>
  </aside>
  <!-- The sidebar's background -->
  <!-- This div must placed right after the sidebar for it to work-->
  <div class="control-sidebar-bg"></div>
  <!-- /.control-sidebar -->
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
</div>
<!-- ./wrapper -->
